{
    "name": "pderas/laravel-azure-ad-oauth",
    "description": "Provides single-sign-on ability to Microsoft Azure Active Directory enabled apps. This package is updated to work with Laravel 8, as the forked package had depency issues with guzzle.",
    "require": {
        "laravel/framework": ">5.4",
        "laravel/socialite": ">3.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "authors": [
        {
            "name": "Patrick Stephan"
        },
        {
            "name": "Brock Roadhouse",
            "email": "brock.roadhouse@fabledsolutions.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pderas\\AzureSocialite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pderas\\AzureSocialite\\ServiceProvider"
            ],
            "aliases": {
                "AzureUser": "Pderas\\AzureSocialite\\AzureUserFacade"
            }
        }
    }
}
